              <ProjectItem
                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[#Region "Imports"
Imports System.Data.SqlClient
Imports System.IO
Imports System.Text
#End Region

'ScriptMain is the entry point class of the script.  Do not change the name, attributes,
'or parent of this class.
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute()> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    'This method is called when this script task executes in the control flow.
    'Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
    'To open Help, press F1.

    Private Const DataFileName As String = "RFQHeader_{0}.txt"

    Private connectionString As String = String.Empty
    Private commandTimeout As Integer = 0
    Private addDay As Integer = 0
    Private tempFilePath As String = String.Empty
    Private outputFilePath As String = String.Empty

    ''' <summary>
    ''' メイン処理を行います。
    ''' </summary>
    Public Sub Main()

        Dts.TaskResult = ScriptResults.Success

        connectionString = Dts.Variables("ConnectionString").Value.ToString
        commandTimeout = Convert.ToInt32(Dts.Variables("CommandTimeout").Value)
        addDay = Convert.ToInt32(Dts.Variables("AddDay").Value)
        tempFilePath = Path.Combine(Dts.Variables("TempDir").Value.ToString, String.Format(DataFileName, ""))
        outputFilePath = Path.Combine(Dts.Variables("OutputDir").Value.ToString, String.Format(DataFileName, Now.ToString("yyyyMMdd")))

        Dim sqlStr As New StringBuilder
        sqlStr.AppendLine("SELECT")
        sqlStr.AppendLine("    *")
        sqlStr.AppendLine("FROM")
        sqlStr.AppendLine("    v_RFQHeaderForPPQM")
        If addDay < 0 Then
            sqlStr.AppendLine("WHERE")
            sqlStr.AppendLine("    UpdateDate >= CONVERT(date, DATEADD(DAY, @AddDay, GETDATE()))")
        End If
        sqlStr.AppendLine("ORDER BY")
        sqlStr.AppendLine("    RFQNumber")

        Try
            If File.Exists(tempFilePath) Then
                File.Delete(tempFilePath)
            End If

            Using sqlConn As SqlConnection = New SqlConnection(connectionString)
                sqlConn.Open()

                Using sqlCmd As SqlCommand = sqlConn.CreateCommand
                    sqlCmd.CommandText = sqlStr.ToString
                    sqlCmd.Parameters.Add("AddDay", SqlDbType.Int)
                    sqlCmd.Parameters("AddDay").Value = addDay
                    sqlCmd.CommandTimeout = commandTimeout

                    Using sqlReader As SqlDataReader = sqlCmd.ExecuteReader

                        Using swTsv As New StreamWriter(tempFilePath, True, Encoding.UTF8)

                            Dim colName As String = String.Empty

                            For i As Integer = 0 To sqlReader.FieldCount - 1
                                colName = sqlReader.GetName(i)
                                swTsv.Write(colName)
                                If i < sqlReader.FieldCount - 1 Then
                                    swTsv.Write(vbTab)
                                End If
                            Next
                            swTsv.Write(vbCrLf)

                            Do Until sqlReader.Read = False

                                Dim colData As String = String.Empty

                                For i As Integer = 0 To sqlReader.FieldCount - 1
                                    colData = RemoveTabString(sqlReader.GetValue(i).ToString)
                                    swTsv.Write(colData)
                                    If i < sqlReader.FieldCount - 1 Then
                                        swTsv.Write(vbTab)
                                    End If
                                Next
                                swTsv.Write(vbCrLf)
                            Loop

                        End Using
                    End Using
                End Using
            End Using

            File.Copy(tempFilePath, outputFilePath, True)

        Catch ex As Exception

            Dim errStr As New StringBuilder
            errStr.AppendLine(Dts.Variables("ErrorMessage").Value.ToString)
            errStr.AppendLine(String.Format("【見積ヘッダデータ出力 ({0}) に失敗しました】", Path.GetFileName(outputFilePath)))
            errStr.AppendLine("エラーメッセージ：")
            errStr.AppendLine(ex.ToString)

            Dts.Variables("ErrorMessage").Value = errStr.ToString

            Dts.TaskResult = ScriptResults.Failure

        End Try

    End Sub

    ''' <summary>
    ''' 文字列からタブを除去します。
    ''' </summary>
    ''' <param name="str">文字列</param>
    ''' <returns>タブを除去した文字列</returns>
    Function RemoveTabString(ByVal str As String) As String

        Return str.Replace(vbTab, String.Empty)

    End Function

#Region "ScriptResults declaration"
    'This enum provides a convenient shorthand within the scope of this class for setting the
    'result of the script.

    'This code was generated automatically.
    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

#End Region

End Class]]></ProjectItem>
