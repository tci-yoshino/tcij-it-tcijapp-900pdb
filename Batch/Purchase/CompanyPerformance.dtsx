          <ProjectItem
            Name="ScriptMain.vb"
            Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic 2008.
' The ScriptMain is the entry point class of the script.

Imports System
Imports System.Collections.Generic
Imports System.Data.SqlClient
Imports System.IO
Imports System.Reflection
Imports System.Text
Imports System.Text.RegularExpressions
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum

    Dim connectionString As String = String.Empty
    Dim commandTimeout As Integer = 0
    Dim outputFilePath As String = String.Empty

    ''' <summary>
    ''' メイン処理を行います。
    ''' </summary>
    Public Sub Main()

        connectionString = Dts.Variables("ConnectionString").Value.ToString
        commandTimeout = CInt(Dts.Variables("CommandTimeout").Value.ToString)
        outputFilePath = String.Format(Dts.Variables("OutputFilePath").Value.ToString, Format(Now(), "yyyy.MM.dd"))

        Dim sqlStr As New StringBuilder
        Dim sqlList As List(Of String) = New List(Of String)
        Dim sheetList As List(Of String) = New List(Of String)

        Try
            'ファイルが既に存在する場合は削除する
            If File.Exists(outputFilePath) Then
                File.Delete(outputFilePath)
            End If

            '先月分の PO Correspondence/History を出力する
            sqlStr = New StringBuilder
            sqlStr.AppendLine("WITH Corres AS (")
            sqlStr.AppendLine("    SELECT")
            sqlStr.AppendLine("        PONumber,")
            sqlStr.AppendLine("        POStatusCode,")
            sqlStr.AppendLine("        MIN(StatusChangeDate) AS StatusChangeDate,")
            sqlStr.AppendLine("        DATEDIFF(MONTH, MIN(StatusChangeDate), GETDATE()) AS MonthDiff")
            sqlStr.AppendLine("    FROM")
            sqlStr.AppendLine("        POHistory")
            sqlStr.AppendLine("    GROUP BY")
            sqlStr.AppendLine("        PONumber,")
            sqlStr.AppendLine("        POStatusCode")
            sqlStr.AppendLine(")")
            sqlStr.AppendLine("SELECT")
            sqlStr.AppendLine("    PO.R3PONumber AS PONumber,")
            sqlStr.AppendLine("    PO.R3POLineNumber AS POLineNumber,")
            sqlStr.AppendLine("    P.ProductNumber,")
            sqlStr.AppendLine("    PO.Priority,")
            sqlStr.AppendLine("    PP.Text AS Purpose,")
            sqlStr.AppendLine("    PO.SupplierCode [Par-PO SupplierCode],")
            sqlStr.AppendLine("    S.R3SupplierCode AS [Par-PO TCI-J SupplierCode],")
            sqlStr.AppendLine("    LTRIM(ISNULL(RTRIM(LTRIM(S.Name3)), '') + ' ' + ISNULL(RTRIM(LTRIM(S.Name4)), '')) AS [Par-PO SupplierName],")
            sqlStr.AppendLine("    PU.Name AS [Par-PO User],")
            sqlStr.AppendLine("    PL.Name AS [Par-PO Location],")
            sqlStr.AppendLine("    CPO.R3PONumber AS [Chi-PO PONumber],")
            sqlStr.AppendLine("    CPO.R3POLineNumber AS [Chi-PO POLineNumber],")
            sqlStr.AppendLine("    CPO.SupplierCode [Chi-PO SupplierCode],")
            sqlStr.AppendLine("    CS.R3SupplierCode AS [Chi-PO TCI-J SupplierCode],")
            sqlStr.AppendLine("    LTRIM(ISNULL(RTRIM(LTRIM(CS.Name3)), '') + ' ' + ISNULL(RTRIM(LTRIM(CS.Name4)), '')) AS [Chi-PO SupplierName],")
            sqlStr.AppendLine("    CU.Name AS [Chi-PO User],")
            sqlStr.AppendLine("    CL.Name AS [Chi-PO Location],")
            sqlStr.AppendLine("    CONVERT(DATE, PPI.StatusChangeDate) AS [Par-PO Issued],")
            sqlStr.AppendLine("    CONVERT(DATE, CPI.StatusChangeDate) AS [Chi-PO Issued],")
            sqlStr.AppendLine("    CONVERT(DATE, PPC.StatusChangeDate) AS [Par-PO Cancelled]")
            sqlStr.AppendLine("FROM")
            sqlStr.AppendLine("    s_Location AS PL,")
            sqlStr.AppendLine("    v_UserAll AS PU,")
            sqlStr.AppendLine("    Supplier AS S,")
            sqlStr.AppendLine("    Product AS P,")
            sqlStr.AppendLine("    RFQHeader AS RH,")
