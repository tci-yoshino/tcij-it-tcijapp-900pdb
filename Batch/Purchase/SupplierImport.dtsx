<![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic
' The ScriptMain class is the entry point of the Script Task.

Imports System
Imports System.Xml
Imports System.Data
Imports System.Math
Imports System.Text.RegularExpressions
Imports Microsoft.SqlServer.Dts.Runtime
Imports System.Data.SqlClient

'*&---------------------------------------------------------------------*
'*& プログラム：接続マネージャ指定タスク    　　                        *
'*& 作成者　　：hashimoto / tci                                         *
'*& 作成日付　：2008/05/30                                              *
'*& 管理番号　：                                                        *
'*& 機能番号　：                                                        *
'*&---------------------------------------------------------------------*
'*& 処理概要&                                                           *
'*&    1) 接続マネージャーのConnectionString値設定                      *
'*&    2) Main処理　　　　　　　　　　　　　　　　　　　　              *
'*&    3) Eorror処理                                                    *
'*&                                                                     *
'*&---------------------------------------------------------------------*
'*& 更新履歴&                                                           *
'*& 更新日付　更新者　　更新内容                                        *
'*&                                                                     *
'*&---------------------------------------------------------------------*


<System.AddIn.AddIn("ScriptMain", Version:="1.0", Publisher:="", Description:="")> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum


    Public Sub Main()

        Dim DBConnString As String '接続文字列	
        Dim DBConn As New System.Data.SqlClient.SqlConnection   'データベースコネクション	
        Dim DBCommand As System.Data.SqlClient.SqlCommand       'データベースコマンド		
        Dim MyCom As SqlCommand
        Dts.Variables("Start_Time").Value = Now()
        ' 変数設定(ログファイル削除時に使用)
        Dim deldate As Integer = CInt(Format(DateAdd(DateInterval.WeekOfYear, -1, Now()), "yyyyMMdd"))
        Dim delFI As IO.FileInfo
        Dim delfile As String = String.Empty

        ' ログ保存ディレクトリから拡張子 .log のファイルを取得し、
        ' ログファイル名形式とマッチ且つ一週間以上前のファイルを削除。
        For Each path As String In IO.Directory.GetFileSystemEntries(Dts.Variables("Dir_Log").Value.ToString, "*.log")

            delFI = New IO.FileInfo(path)
            delfile = Split(delFI.Name, delFI.Extension)(0)

            If Regex.IsMatch(delfile, "^[0-9]{4}[0-9]{2}[0-9]{2}$") Then
                If CInt(delfile) < deldate Then
                    delFI.Delete()
                End If
            End If

        Next path

        '*&---------------------------------------------------------------------*
        '*& SQL接続                                                             *
        '*&---------------------------------------------------------------------*
        Try
            DBConn = _
                DirectCast(Dts.Connections("ADO.Purchase").AcquireConnection(Dts.Transaction), _
                SqlClient.SqlConnection)
            DBCommand = DBConn.CreateCommand()

            '***** 2) Main処理 *****
            '*&---------------------------------------------------------------------*
            '*& ストアドプロシジャー実行                                            *
            '*&---------------------------------------------------------------------*
            MyCom = New SqlCommand("sp_SupplierImport", DBConn)                           'コマンド定義 
            MyCom.CommandType = CommandType.StoredProcedure                               'コマンドタイプをストアドプロシージャに指定
            Dim RETURN_VALUE As SqlParameter = MyCom.Parameters.Add("RETURN_VALUE", SqlDbType.Int) 'ストアドプロシージャからRETURN値(@RETURN_VALUE)を取得する変数
            Dim ErrNUMBER As SqlParameter = MyCom.Parameters.Add("ErrNUMBER", SqlDbType.Int)
            Dim ErrSTATE As SqlParameter = MyCom.Parameters.Add("ErrSTATE", SqlDbType.Int)
            Dim ErrLINE As SqlParameter = MyCom.Parameters.Add("ErrLINE", SqlDbType.Int)
            Dim ErrMESSAGE As SqlParameter = MyCom.Parameters.Add("ErrMESSAGE", SqlDbType.NVarChar, 2000)
            Dim ErrPCOUNTRY As SqlParameter = MyCom.Parameters.Add("ErrPCOUNTRY", SqlDbType.NVarChar, 2000)

            RETURN_VALUE.Direction = ParameterDirection.ReturnValue
            ErrNUMBER.Direction = ParameterDirection.Output
            ErrSTATE.Direction = ParameterDirection.Output
            ErrLINE.Direction = ParameterDirection.Output
            ErrMESSAGE.Direction = ParameterDirection.Output
            ErrPCOUNTRY.Direction = ParameterDirection.Output

            MyCom.CommandTimeout = CInt(Dts.Variables("SPTimeOut").Value)
            MyCom.ExecuteNonQuery()                                                        'ストアドプロシージャの実行

            '***** 3) Eorror処理 *****
            If CInt(RETURN_VALUE.Value) = 0 Then                  'ストアドプロシージャ戻り値チェック
                If Not IsDBNull(ErrPCOUNTRY.Value) Then
                    Dts.Variables("Comment").Value = CStr(ErrPCOUNTRY.Value) & vbCrLf
                    Dts.Variables("Err_Flag").Value = True
                End If
                Dts.TaskResult = ScriptResults.Success
            Else
                Dts.Variables("Comment").Value = "ERROR NUMBER  : " & CStr(ErrNUMBER.Value) & Chr(13) & Chr(10) _
                                               & "ERROR STATE   : " & CStr(ErrSTATE.Value) & Chr(13) & Chr(10) _
                                               & "ERROR LINE    : " & CStr(ErrLINE.Value) & Chr(13) & Chr(10) _
                                               & "ERROR MESSAGE : " & CStr(ErrMESSAGE.Value) & Chr(13) & Chr(10)
                Dts.Variables("Err_Flag").Value = True
                Dts.TaskResult = ScriptResults.Failure
            End If
            MyCom.Dispose() '開放処理

        Catch ex As Exception
            Dts.Variables("Comment").Value = "Connectioninformation : " & Dts.Connections("ADO.Purchase").ConnectionString & Chr(13) & Chr(10) _
                                           & "Error Message : " & ex.Message & Chr(13) & Chr(10)
            Dts.Variables("Err_Flag").Value = True
            Dts.TaskResult = ScriptResults.Failure
        Finally

            DBConn.Close()
            DBConn.Dispose()
        End Try

    End Sub

End Class]]></ProjectItem><ProjectItem Name="\my project\settings.settings">
