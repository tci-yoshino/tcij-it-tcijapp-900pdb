        Public Function GetSupplierInfo(ByVal st_SupplierCode As String,  ByVal st_RoleCode As String) As List(Of TCIDataAccess.Join.RFQIssueDispSupplierInfo)
            Dim supplierInfoList As List(Of TCIDataAccess.Join.RFQIssueDispSupplierInfo) = New List(Of TCIDataAccess.Join.RFQIssueDispSupplierInfo)

            'データベースからデータを読み込む SQL 文字列を生成する。
            Dim sb_SQL As New StringBuilder()

        sb_Sql.AppendLine("WITH IrregularQuoLocation AS ( ")
        sb_Sql.AppendLine("  SELECT ")
        sb_Sql.AppendLine("    I.[SupplierCode], ")
        sb_Sql.AppendLine("    I.[QuoLocationCode] AS QuoLocationCode, ")
        sb_Sql.Append("    ISNULL(L.[Name], '") 
        sb_Sql.Append(common.DIRECT)
        sb_Sql.AppendLine("') AS QuoLocationName ")
        sb_Sql.AppendLine("  FROM ")
        sb_Sql.AppendLine("    [IrregularRFQLocation] AS I ")
        sb_Sql.AppendLine("      LEFT OUTER JOIN [s_Location] AS L ")
        sb_Sql.AppendLine("        ON L.[LocationCode] = I.[QuoLocationCode] ")
        sb_Sql.AppendLine(")")
        sb_Sql.AppendLine("SELECT ")
        sb_Sql.AppendLine("  S.[S4SupplierCode], ")
        sb_Sql.AppendLine("  S.[SupplierCode], ")
        sb_Sql.AppendLine("  S.[R3SupplierCode], ")
        sb_Sql.AppendLine("  S.[CountryCode], ")
        sb_Sql.AppendLine("  LTRIM(RTRIM(ISNULL(S.[Name3], '') + ' ' + ISNULL(S.[Name4], ''))) AS Name, ")
        sb_Sql.AppendLine("  C.[CountryName] AS CountryName, ")
        sb_Sql.AppendLine("  CASE WHEN I.QuoLocationName IS NULL THEN C.[DefaultQuoLocationCode] ELSE I.[QuoLocationCode] END AS QuoLocationCode, ")
        sb_Sql.AppendLine("  ISNULL(I.QuoLocationName, C.[DefaultQuoLocationName]) AS QuoLocationName ")
        sb_Sql.AppendLine("FROM ")
        sb_Sql.AppendLine("  [Supplier] AS S ")
        sb_Sql.AppendLine("    LEFT OUTER JOIN IrregularQuoLocation AS I ")
        sb_Sql.AppendLine("      ON S.[SupplierCode] = I.SupplierCode ")
        sb_Sql.AppendLine("    LEFT OUTER JOIN [v_Country] AS C ")
        sb_Sql.AppendLine("      ON S.[CountryCode] = C.[CountryCode] ")
        sb_Sql.AppendLine("WHERE ")
        sb_Sql.AppendLine("  S.[SupplierCode] = @SupplierCode ")

            Using DBConn As New SqlConnection(DB_CONNECT_STRING)
                Using DBCommand As SqlCommand = DBConn.CreateCommand()
                    DBCommand.CommandText = sb_SQL.ToString()
                    DBCommand.Parameters.AddWithValue("SupplierCode", st_SupplierCode)
                    DBConn.Open()
                    Dim DBReader As SqlDataReader = DBCommand.ExecuteReader()

                    While DBReader.Read()

                        Dim dc_SupplierInfo As TCIDataAccess.Join.RFQIssueDispSupplierInfo = New TCIDataAccess.Join.RFQIssueDispSupplierInfo

                        SetProperty(DBReader("S4SupplierCode"), dc_SupplierInfo.S4SupplierCode)
                        SetProperty(DBReader("SupplierCode"), dc_SupplierInfo.SupplierCode)
                        SetProperty(DBReader("R3SupplierCode"), dc_SupplierInfo.R3SupplierCode)
                        SetProperty(DBReader("CountryCode"), dc_SupplierInfo.CountryCode)
                        SetProperty(DBReader("Name"), dc_SupplierInfo.Name)
                        SetProperty(DBReader("CountryName"), dc_SupplierInfo.CountryName)
                        SetProperty(DBReader("QuoLocationCode"), dc_SupplierInfo.QuoLocationCode)
                        SetProperty(DBReader("QuoLocationName"), dc_SupplierInfo.QuoLocationName)

                        supplierInfoList.Add(dc_SupplierInfo)

                    End While

                    DBReader.Close()

                End Using
            End Using

            Return supplierInfoList

        End Function
        Public Function GetMakerInfo(ByVal st_MakerCode As String,  ByVal st_RoleCode As String) As List(Of TCIDataAccess.Join.RFQIssueDispMakerInfo)
            Dim makerInfoList As List(Of TCIDataAccess.Join.RFQIssueDispMakerInfo) = New List(Of TCIDataAccess.Join.RFQIssueDispMakerInfo)

