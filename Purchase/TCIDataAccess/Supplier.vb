            Dim sb_SQL As New StringBuilder()

            sb_Sql.AppendLine("SELECT ")
            sb_Sql.AppendLine("  S.[S4SupplierCode], ")
            sb_Sql.AppendLine("  S.[SupplierCode], ")
            sb_Sql.AppendLine("  S.[LocationCode], ")
            sb_Sql.AppendLine("  SC.[Name] AS CountryName, ") 
            sb_Sql.AppendLine("  LTRIM(RTRIM(ISNULL(S.[Name3], '') + ' ' + ISNULL(S.[Name4], ''))) AS Name ")
            sb_Sql.AppendLine("FROM ")
            sb_Sql.AppendLine("  [Supplier] AS S ")
            sb_Sql.AppendLine("    LEFT OUTER JOIN [s_Country] AS SC ")
            sb_Sql.AppendLine("      ON S.[CountryCode] = SC.[CountryCode] ")
            sb_Sql.AppendLine("WHERE ")
            sb_Sql.AppendLine("  S.[SupplierCode] = @SupplierCode ")
            sb_Sql.AppendLine("ORDER BY [SupplierCode], [Name3] ")

            Using DBConn As New SqlConnection(DB_CONNECT_STRING)
                Using DBCommand As SqlCommand = DBConn.CreateCommand()
                    DBCommand.CommandText = sb_SQL.ToString()
                    DBCommand.Parameters.AddWithValue("SupplierCode", st_MakerCode)
