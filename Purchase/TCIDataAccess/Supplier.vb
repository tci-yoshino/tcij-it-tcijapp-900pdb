        ''' <summary>
        ''' Product List By SupplierのHeader情報を取得します。
        ''' </summary>
        ''' <remarks></remarks>
        ''' <param name="st_SupplierCode">SupplierCode</param>
        ''' <returns>ProductListBySupplierDispHeader</returns>
        Public Shared Function GetProductListBySupplierHeader(st_SupplierCode As String) As TCIDataAccess.Join.ProductListBySupplierDispHeader
            Dim productListBySupplierDispHeader As TCIDataAccess.Join.ProductListBySupplierDispHeader = new TCIDataAccess.Join.ProductListBySupplierDispHeader

            'データベースからデータを読み込む SQL 文字列を生成する。
            Dim sb_SQL As StringBuilder = New StringBuilder

            sb_SQL.AppendLine("SELECT DISTINCT ")
            sb_SQL.AppendLine("  [Name3], ")
            sb_SQL.AppendLine("  [Name4], ")
            sb_SQL.AppendLine("  CASE ")
            sb_SQL.AppendLine("    WHEN ")
            sb_SQL.AppendLine("      IRSL.QuoLocationCode Is NULL ")
            sb_SQL.AppendLine("    THEN ")
            sb_SQL.AppendLine("      [V_Country].[DefaultQuoLocationName] ")
            sb_SQL.AppendLine("    ELSE ")
            sb_SQL.AppendLine("      IRSL.[Name] ")
            sb_SQL.AppendLine("    END AS Territory ")
            sb_SQL.AppendLine("FROM [Supplier] AS S ")
            sb_SQL.AppendLine("  LEFT OUTER JOIN ( ")
            sb_SQL.AppendLine("    SELECT ")
            sb_SQL.AppendLine("      IRL.[SupplierCode], ")
            sb_SQL.AppendLine("      IRL.[QuoLocationCode], ")
            sb_SQL.AppendLine("      SL.[Name] ")
            sb_SQL.AppendLine("    FROM [IrregularRFQLocation] AS IRL ")
            sb_SQL.AppendLine("      LEFT OUTER JOIN [s_Location] AS SL ")
            sb_SQL.AppendLine("        ON IRL.[QuoLocationCode] = SL.[LocationCode] ")
            sb_SQL.AppendLine("  ) IRSL ")
            sb_SQL.AppendLine("  ON S.[SupplierCode] = IRSL.SupplierCode ")
            sb_SQL.AppendLine("  LEFT OUTER JOIN [Supplier_Product] AS SP ")
            sb_SQL.AppendLine("    ON SP.[SupplierCode] = S.[SupplierCode] ")
            sb_SQL.AppendLine("  LEFT OUTER JOIN [v_Country] ")
            sb_SQL.AppendLine("    ON [v_Country].[CountryCode] = S.[CountryCode] ")
            sb_SQL.AppendLine("  WHERE ( ")
            sb_SQL.AppendLine("    S.[SupplierCode] = @SupplierCode) ")

            Using DBConn As New SqlConnection(Common.DB_CONNECT_STRING)
                Using DBCommand As SqlCommand = DBConn.CreateCommand()
                    DBCommand.Parameters.Clear()
                    DBCommand.Parameters.AddWithValue("SupplierCode", CInt(st_SupplierCode))
                    DBCommand.CommandText = sb_SQL.ToString()

                    ' 実行
                    DBConn.Open()
                    Dim DBReader As SqlDataReader = DBCommand.ExecuteReader()

                    While DBReader.Read

                        Dim dc_ProductListBySupplierDispHeader As TCIDataAccess.Join.ProductListBySupplierDispHeader = New TCIDataAccess.Join.ProductListBySupplierDispHeader

                        DBCommon.SetProperty(DBReader("Name3"), dc_ProductListBySupplierDispHeader.Name3)
                        DBCommon.SetProperty(DBReader("Name4"), dc_ProductListBySupplierDispHeader.Name4)
                        DBCommon.SetProperty(DBReader("Territory"), dc_ProductListBySupplierDispHeader.Territory)

                        productListBySupplierDispHeader = dc_ProductListBySupplierDispHeader

                    End While

                    DBConn.Close

                End Using
            End Using

            Return productListBySupplierDispHeader

        End Function

        ''' <summary>
        ''' Supplier Contact Personのドロップダウンリストに値を設定するためのSQL文を返却します。
        ''' </summary>
        ''' <remarks></remarks>
        ''' <param name="st_SupplierCode">SupplierCode</param>
        ''' <returns>DropDownListItems</returns>
        Public Shared Function GetSupplierContactPersonCodeDropDownList(st_SupplierCode As String) As List(Of DropDownListItems)
            Dim dropDownListItems As List(Of DropDownListItems) = New List(Of DropDownListItems)

            Dim sb_SQL As StringBuilder = New StringBuilder

            sb_SQL.AppendLine("SELECT ")
            sb_SQL.AppendLine("  TBL.ItemValue, TBL.ItemText, ROW_NUMBER() OVER(ORDER BY ItemValue ASC) AS ItemOrder ")
            sb_SQL.AppendLine("FROM (")
            sb_SQL.AppendLine("  SELECT ")
            sb_SQL.AppendLine("    T.ItemValue, T.ItemText ")
            sb_SQL.AppendLine("  FROM (")
            sb_SQL.AppendLine("    SELECT [SupplierEmailID1] AS ItemValue, [SupplierEmailID1] + ' - ' + ISNULL([SupplierContactperson1], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID1] <> '' AND [SupplierEmailID1] IS NOT NULL AND [SupplierEmailID1] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID2] AS ItemValue, [SupplierEmailID2] + ' - ' + ISNULL([SupplierContactperson2], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID2] <> '' AND [SupplierEmailID2] IS NOT NULL AND [SupplierEmailID2] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID3] AS ItemValue, [SupplierEmailID3] + ' - ' + ISNULL([SupplierContactperson3], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID3] <> '' AND [SupplierEmailID3] IS NOT NULL AND [SupplierEmailID3] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID4] AS ItemValue, [SupplierEmailID4] + ' - ' + ISNULL([SupplierContactperson4], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID4] <> '' AND [SupplierEmailID4] IS NOT NULL AND [SupplierEmailID4] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID5] AS ItemValue, [SupplierEmailID5] + ' - ' + ISNULL([SupplierContactperson5], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID5] <> '' AND [SupplierEmailID5] IS NOT NULL AND [SupplierEmailID5] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID6] AS ItemValue, [SupplierEmailID6] + ' - ' + ISNULL([SupplierContactperson6], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID6] <> '' AND [SupplierEmailID6] IS NOT NULL AND [SupplierEmailID6] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID7] AS ItemValue, [SupplierEmailID7] + ' - ' + ISNULL([SupplierContactperson7], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID7] <> '' AND [SupplierEmailID7] IS NOT NULL AND [SupplierEmailID7] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID8] AS ItemValue, [SupplierEmailID8] + ' - ' + ISNULL([SupplierContactperson8], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID8] <> '' AND [SupplierEmailID8] IS NOT NULL AND [SupplierEmailID8] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID9] AS ItemValue, [SupplierEmailID9] + ' - ' + ISNULL([SupplierContactperson9], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID9] <> '' AND [SupplierEmailID9] IS NOT NULL AND [SupplierEmailID9] <> '000' ")
            sb_SQL.AppendLine("    UNION ALL SELECT [SupplierEmailID10] AS ItemValue, [SupplierEmailID10] + ' - ' + ISNULL([SupplierContactperson10], '') AS ItemText FROM [Supplier] WHERE [SupplierCode] = @SupplierCode AND [SupplierEmailID10] <> '' AND [SupplierEmailID10] IS NOT NULL AND [SupplierEmailID10] <> '000' ")
            sb_SQL.AppendLine("  ) AS T")
            sb_SQL.AppendLine(") AS TBL")

            Using DBConn As New SqlConnection(DB_CONNECT_STRING)
                Using DBCommand As SqlCommand = DBConn.CreateCommand()
                    DBCommand.CommandText = sb_SQL.ToString()
                    DBCommand.Parameters.Clear()
                    DBCommand.Parameters.AddWithValue("SupplierCode", CInt(st_SupplierCode))
                    DBConn.Open()

                    Dim DBReader As SqlDataReader = DBCommand.ExecuteReader()

                    If DBReader.HasRows Then
                        While DBReader.Read
                            Dim dc_DropDownListItems As DropDownListItems = New DropDownListItems

                            DBCommon.SetProperty(DBReader("ItemValue"), dc_DropDownListItems.ItemValue)
                            DBCommon.SetProperty(DBReader("ItemText"), dc_DropDownListItems.ItemText)
                            DBCommon.SetProperty(DBReader("ItemOrder"), dc_DropDownListItems.ItemOrder)
                            dropDownListItems.Add(dc_DropDownListItems)

                        End While

                    'Else
                    '    Dim dc_DropDownListItems As DropDownListItems = New DropDownListItems

                    '    DBCommon.SetProperty(String.Empty, dc_DropDownListItems.ItemValue)
                    '    DBCommon.SetProperty(String.Empty, dc_DropDownListItems.ItemText)
                    '    DBCommon.SetProperty(0, dc_DropDownListItems.ItemOrder)
                    '    dropDownListItems.Add(dc_DropDownListItems)

                    End If

                    DBConn.Close

                    Return dropDownListItems

                End Using
            End Using

